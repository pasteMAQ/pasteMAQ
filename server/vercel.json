{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/view-paste/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/validate-paste/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/validate-pin/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/paste",
      "dest": "index.js"
    }
  ]
}